score = float(input('Enter Score: '))

if score >= 0.9:
    print('Grade: A')
else:
   if score >=0.8:
       print('Grade: B')
   else:
       if score >=0.7:
           print('Grade: C')
       else:
           if score >=0.6:
               print('Grade: D')
           else:
               if score <0.6:
                   print('Grade F')
               else:
                   float(print('Bad Score'))
                   if score >=1.0:
                       float(print('Bad Score'))    
